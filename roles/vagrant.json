{
  "name" : "vagrant",
  "chef_type": "role",
  "json_class" : "Chef::Role",
  "description" : "Vagrant role",
  "run_list": [
    "role[base]",
    "recipe[rvm::install]",
    "recipe[virtualbox]",
    "recipe[vagrant_boxes]"
    ],
  "default_attributes": {
    "rvm": {
      "ruby": {
        "version": "1.9.3",
        "default": "true"
      }
    }
 }
}
